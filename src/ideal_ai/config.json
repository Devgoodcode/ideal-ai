      "formatter": "_format_for_ollama_vision",
      "caller": "_call_ollama",
      "parser": "_parse_ollama",
      "url_template": "$ollama_url/api/chat"
    },
    
    "ollama_image_gen": {
      "formatter": "_format_for_ollama_image_gen", 
      "caller": "_call_ollama",
      "parser": "_parse_ollama_image", 
      "url_template": "$ollama_url/api/generate",
      "timeout": 180
    },
    
    "openai_vision_compatible": {
      "formatter": "_format_for_openai_vision_style",
      "caller": "_call_http_post",
      "parser": "_parse_openai_style",
      "headers_template": {
        "Authorization": "Bearer $api_key",
        "Content-Type": "application/json"
      }
    },
    "infomaniak_whisper": {
      "formatter": "_format_for_whisper",
      "caller": "_call_and_poll_infomaniak_whisper",
      "parser": "_parse_whisper_final_response",
      "headers_template": {
        "Authorization": "Bearer $api_key"
      },
      "url_template": "https://api.infomaniak.com/1/ai/$infomaniak_product/openai/audio/transcriptions"
    },
    "openai_tts_api": {
      "formatter": "_format_for_openai_tts",
      "caller": "_call_http_post_binary",
      "parser": "_parse_audio_output",
      "headers_template": {
        "Authorization": "Bearer $api_key",
        "Content-Type": "application/json"
      }
    },
    "openai_image_gen_api": {
      "formatter": "_format_for_image_gen",
      "caller": "_call_http_post",
      "parser": "_parse_image_gen_response",
      "headers_template": {
        "Authorization": "Bearer $api_key",
        "Content-Type": "application/json"
      }
    },
    "alibaba_video_gen": {
      "formatter": "_format_for_alibaba_video_gen",
      "caller": "_call_and_poll_alibaba_video",
      "parser": "_parse_alibaba_video_gen",
      "headers_template": {
        "Authorization": "Bearer $api_key",
        "Content-Type": "application/json",
        "X-DashScope-Async": "enable"
      },
      "url_template": "https://dashscope-intl.aliyuncs.com/api/v1/services/aigc/video-generation/video-synthesis",
      "poll_url_template": "https://dashscope-intl.aliyuncs.com/api/v1/tasks/$task_id"
    }
  },


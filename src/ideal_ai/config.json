      "formatter": "_format_for_qwen_image_generation",
      "caller": "_call_http_post", 
      "parser": "_parse_image_gen_response",
      "headers_template": {
        "Authorization": "Bearer $api_key",
        "Content-Type": "application/json"
      },
      "url_template": "https://dashscope-intl.aliyuncs.com/api/v1/services/aigc/multimodal-generation/generation"
    },

    "openai_compatible": {
      "formatter": "_format_for_openai_style",

        "Content-Type": "application/json"
      },
      "url_template": "https://api-inference.huggingface.co/models/$model_id"
    },
    "anthropic_api": {
      "formatter": "_format_for_anthropic",
      "caller": "_call_http_post",
      "parser": "_parse_anthropic",
      "headers_template": {
        "x-api-key": "$api_key",
        "anthropic-version": "2023-06-01",
        "Content-Type": "application/json"
      },
      "url_template": "https://api.anthropic.com/v1/messages"
    },
    "google_sdk_vision": {
      "formatter": "_format_for_google_vision",
